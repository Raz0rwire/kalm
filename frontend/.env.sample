# https://create-react-app.dev/docs/setting-up-your-editor/#experimental-extending-the-eslint-config
EXTEND_ESLINT=true

# api prefix
REACT_APP_K8S_API_PERFIX=http://localhost:3010

# api version
REACT_APP_K8S_API_VERSION=v1alpha1

# use mock api backend for development and test
REACT_APP_USE_MOCK_API=false

# display some debug info/component; register debug middlewares/plugins
REACT_APP_DEBUG=true